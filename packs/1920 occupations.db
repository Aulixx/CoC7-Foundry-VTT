{"_id":"1Osh6EdNSjpjahDF","name":"Beggar","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You devote your life to begging food and salable items from passersby.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":2,"selected":true,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":2,"selected":true,"optional":false,"active":false},"edu":{"multiplier":null,"selected":false,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.art-craft-acting","i.skill.stealth","i.skill.insight"],"groups":[{"options":"1","skills":["i.skill.listen","i.skill.spot-hidden"]},{"options":"1","skills":["i.skill.charm","i.skill.fast-talk","i.skill.intimidate","i.skill.persuade"]}],"personal":2,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.beggar","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"2Osh6EdNSjpjahDF","name":"Cleric","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You are the child of a rich man, or a brilliant peasant boy who caught the notice of a man of the Church. You received a formal religious education in a bishopric or a monastery. Now you are a secretary, an administrator, a jurist, or an architect at the service of a count or a bishop.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":2,"selected":true,"optional":true,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":2,"selected":true,"optional":true,"active":false},"edu":{"multiplier":2,"selected":true,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.charm","i.skill.library-use","i.skill.persuade","i.skill.language-latin","i.skill.kingdom-own","i.skill.religion","i.skill.reading-and-writing"],"groups":[],"personal":1,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.cleric","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"3Osh6EdNSjpjahDF","name":"Craftsman or Shop Keeper","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You might be a smith, a baker, or a weaver. Choose your craft. You live in a village community or in a city</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":2,"selected":true,"optional":true,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":2,"selected":true,"optional":true,"active":false},"app":{"multiplier":null,"selected":true,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":2,"selected":true,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.accounting","i.skill.charm","i.skill.art-craft-any","i.skill.fast-talk","i.skill.insight","i.skill.natural-world","i.skill.kingdom-own"],"groups":[],"personal":1,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.craftsmanOrShopKeeper","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"4Osh6EdNSjpjahDF","name":"(Free) Farmer","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You are the salt of the earth, a well-to-do farmer or colonist. Society depends on your crops, and you work like a horse.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":2,"selected":true,"optional":false,"active":false},"con":{"multiplier":2,"selected":true,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":null,"selected":false,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.animal-handling","i.skill.art-craft-any","i.skill.drive-horse-oxen","i.skill.listen","i.skill.natural-world","i.skill.navigate","i.skill.track"],"groups":[],"personal":1,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.freeFarmer","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"5Osh6EdNSjpjahDF","name":"Guard","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You work in a cathedral city for the burgrave or the bishop. In times of peace, you have little to do but practice with your weapons and keep in shape</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":2,"selected":true,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":2,"selected":true,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":null,"selected":false,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.fighting-brawl","i.skill.listen","i.skill.kingdom-own","i.skill.ride","i.skill.spot-hidden","i.skill.throw"],"groups":[],"personal":2,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.guard","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"6Osh6EdNSjpjahDF","name":"Healer","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>To foreigners, you look like a villager, but villagers know better: your mentor granted you powers of the invisible world. Now villagers come to your hut for a cure or a potion, or for advice about love or birthing a child, the promise of rain and the evil eye. Be wary of the ever-suspicious village priest!</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":4,"selected":true,"optional":true,"active":false},"edu":{"multiplier":4,"selected":true,"optional":true,"active":false}},"suggestedContacts":"","skills":["i.skill.art-craft-potions","i.skill.first-aid","i.skill.insight","i.skill.listen","i.skill.spot-hidden","i.skill.natural-world","i.skill.occult"],"groups":[],"personal":2,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.healer","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"7Osh6EdNSjpjahDF","name":"Hermit/Heretic","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You are an outcast, a drifter, a person plagued by dreams and visions. You grasp at strange clues and bewildering notions. You either hide in the woods or mountains, or live in a secret community.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":4,"selected":true,"optional":false,"active":false},"edu":{"multiplier":null,"selected":false,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.stealth","i.skill.insight","i.skill.listen","i.skill.spot-hidden","i.skill.natural-world","i.skill.occult","i.skill.persuade","i.skill.religion"],"groups":[],"personal":1,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.hermitHeretic","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"8Osh6EdNSjpjahDF","name":"Household Officer","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You serve your lord in his urban palace or his castrum (military building). Select one of the following functions: steward, headman of the stables, or keeper of order. You spend much of your day bullying lesser servants to do their work.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":2,"selected":true,"optional":false,"active":false},"edu":{"multiplier":2,"selected":true,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.animal-handling","i.skill.art-craft-any","i.skill.fast-talk","i.skill.insight","i.skill.listen","i.skill.spot-hidden"],"groups":[],"personal":2,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.householdOfficer","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"9Osh6EdNSjpjahDF","name":"Juggler/Minstrel","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You’re witty and articulate, dress  gaily, and you love to get attention. You might be adept with  chansons de geste (songs of  heroic deeds)—your heroes  are Roland, Charlemagne  and Alexander the  Great—you play a  musical instrument,  recite poetry and  stories that everybody  already knows, and  are maybe proficient  at tumbling, juggling,  rope walking, or some  other entertaining  craft. You might be a  Goliard, composing  and singing in Latin.  You might even own a tame  bear or monkey! If possible, you enter the service of a nobleman, who praises your singing, and in return you hope  for gifts and treasures, and the security only a court can  offer you.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":2,"selected":true,"optional":false,"active":false},"app":{"multiplier":2,"selected":true,"optional":true,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":2,"selected":true,"optional":true,"active":false},"edu":{"multiplier":null,"selected":false,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.art-craft-any","i.skill.charm","i.skill.fast-talk","i.skill.insight","i.skill.kingdom-own","i.skill.persuade"],"groups":[],"personal":2,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.jugglerMinstrel","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"11sh6EdNSjpjahDF","name":"Mercenary/Brigand","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>As a mercenary you fight for the highest bidder and then scavenge battlefields for trophies. As a brigand, you may have been the victim of some natural catastrophe or some heinous injustice that changed your life forever. Now you hide deep in the woods and rob traveling monks or traders.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":2,"selected":true,"optional":false,"active":false},"con":{"multiplier":2,"selected":true,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":null,"selected":false,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.fighting-brawl","i.skill.ranged-weapons","i.skill.natural-world","i.skill.navigate","i.skill.track","i.skill.stealth","i.skill.throw"],"groups":[],"personal":1,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.mercenaryBrigand","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"12sh6EdNSjpjahDF","name":"Merchant","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You live in a city or on the outskirts of a cathedral city. You make a living from accounts and agents. You import wine, exotic spices,and silks from heathen countries and sell them to arrogant nobles. If you’re not a Christian, you’re allowed to be a moneychanger and a moneylender </p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":4,"selected":true,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.accounting","i.skill.charm","i.skill.fast-talk","i.skill.kingdom-any","i.skill.language-any","i.skill.reading-and-writing"],"groups":[],"personal":2,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.merchant","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"13sh6EdNSjpjahDF","name":"Monk/Nun","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You live in a monastery, in silence and prayer, leading a simple life. When you don’t pray or sing, you perform domestic tasks, or copy arcane manuscripts from the monastery’s library. As a member of your order, you are not allowed to own private property but if you leave the monastery with special permission, you can always count on the hospitality and charity of other monasteries.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":4,"selected":true,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.library-use","i.skill.listen","i.skill.language-latin","i.skill.religion","i.skill.reading-and-writing"],"groups":[{"options":"1","skills":["i.skill.art-craft-any","i.skill.science-any"]}],"personal":2,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.monkNun","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"14sh6EdNSjpjahDF","name":"Pilgrim","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You live by the charity of other people. You accomplish a pilgrimage to a holy place, such as Rome, a monastery, or a cathedral city housing holy relics. You have your own reasons to be a pilgrim, maybe for the expiation of some crime, the wish to elevate your soul, or simply the desire for adventure in its noblest sense. You could be headed to Santiago de Compostela in Spain, Mont Saint Michel in France, Jerusalem, or Rome.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":2,"selected":true,"optional":false,"active":false},"edu":{"multiplier":2,"selected":true,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.charm","i.skill.natural-world","i.skill.navigate","i.skill.language-any","i.skill.kingdom-own","i.skill.religion","i.skill.stealth"],"groups":[],"personal":1,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.pilgrim","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"15sh6EdNSjpjahDF","name":"Priest","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You’re on a mission from the church to enlighten laymen and women in the ways of God. You could be an acolyte, an exorcist, or a fully-fledged priest who is bound to a parish and collects the tithe from the farmers, most of which goes to your greedy lord. Although St. Paul highly commends celibacy, and it is strongly recommended, you may have a concubine and even children, though they will not inherit from you</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":2,"selected":true,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":2,"selected":true,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.insight","i.skill.language-latin","i.skill.occult","i.skill.persuade","i.skill.religion"],"groups":[],"personal":3,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.priest","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"16sh6EdNSjpjahDF","name":"Sailor","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You’re skilled with sails, boats, and ships, and know tides, the wind, and the stars. You have seen Hamburg, Venice, or Constantinople. Life is glorious, except for storms, pirates, and the terrors of the deep</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":2,"selected":true,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":2,"selected":true,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":null,"selected":false,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.climb","i.skill.fast-talk","i.skill.natural-world","i.skill.navigate","i.skill.kingdom-any","i.skill.pilot-boat"],"groups":[],"personal":2,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.sailor","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"17sh6EdNSjpjahDF","name":"Scholar","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You belong to a monastic or cathedral school. You are the recipient and the dispenser of godly knowledge. You spend your time reading classical authors, writing manuals, and teaching. When you don’t teach, you may be involved in political intrigues for some good cause.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":4,"selected":true,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.library-use","i.skill.occult","i.skill.language-latin","i.skill.kingdom-own","i.skill.persuade","i.skill.science-any","i.skill.reading-and-writing"],"groups":[],"personal":1,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.scholar","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"18sh6EdNSjpjahDF","name":"Sergeant/Mayor","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You are employed by a lord or a monastery to supervise the administration of the domain. Your main task is to collect tax money and dues in kind.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":2,"selected":true,"optional":false,"active":false},"con":{"multiplier":null,"selected":false,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":null,"selected":false,"optional":false,"active":false},"pow":{"multiplier":2,"selected":true,"optional":false,"active":false},"edu":{"multiplier":null,"selected":false,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.accounting","i.skill.charm","i.skill.insight","i.skill.reading-and-writing","i.skill.spot-hidden"],"groups":[{"options":"1","skills":["i.skill.fighting-brawl","i.skill.ranged-weapons"]}],"personal":1,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.sergeantMayor","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
{"_id":"19sh6EdNSjpjahDF","name":"Small Trader","type":"occupation","img":"icons/svg/ice-aura.svg","system":{"source":"FoundryVTT CoC7","description":{"value":"<p>You own a few pack animals or a small boat. You circuit inland, up river, or along the coast for the benefit of your master. You know a lot about that route and its particular dangers.</p>"},"type":{"classic":true},"occupationSkillPoints":{"str":{"multiplier":null,"selected":false,"optional":false,"active":false},"con":{"multiplier":2,"selected":true,"optional":false,"active":false},"siz":{"multiplier":null,"selected":false,"optional":false,"active":false},"dex":{"multiplier":null,"selected":false,"optional":false,"active":false},"app":{"multiplier":null,"selected":false,"optional":false,"active":false},"int":{"multiplier":2,"selected":true,"optional":false,"active":false},"pow":{"multiplier":null,"selected":false,"optional":false,"active":false},"edu":{"multiplier":null,"selected":false,"optional":false,"active":false}},"suggestedContacts":"","skills":["i.skill.charm","i.skill.fast-talk","i.skill.insight","i.skill.kingdom-own","i.skill.language-any","i.skill.navigate"],"groups":[{"options":"1","skills":["i.skill.drive-horse-oxen","i.skill.pilot-boat"]}],"personal":1,"personalText":"","attributes":{},"properties":{},"flags":{}},"flags":{"CoC7":{"cocidFlag":{"id":"i.occupation.smallTrader","lang":"en","priority":0,"eras":{"darkAges":true}}}}}
